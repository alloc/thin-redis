{
  "name": "thin-redis",
  "version": "0.11.0",
  "description": "Redis/Valkey client for Node.js and Cloudflare Workers",
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --clean --watch --sourcemap",
    "build": "tsup --clean",
    "test": "vitest",
    "lint": "tsc-lint",
    "prepublishOnly": "pnpm lint && pnpm build"
  },
  "main": "dist/index.mjs",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "redis",
    "cloudflare",
    "workers",
    "typescript",
    "socket",
    "tcp",
    "typebox"
  ],
  "author": "Alec Larson",
  "contributors": [
    "Kane Wang"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alloc/thin-redis.git"
  },
  "type": "module",
  "license": "MIT",
  "peerDependencies": {
    "@arrowood.dev/socket": ">=0.2.0",
    "@sinclair/typebox": "*"
  },
  "peerDependenciesMeta": {
    "@arrowood.dev/socket": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arrowood.dev/socket": "0.2.0",
    "@cloudflare/workers-types": "^4.20241011.0",
    "@sinclair/typebox": "^0.34.21",
    "@types/node": "^22.7.5",
    "jsonpath-ts": "^0.1.1",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "radashi": "13.0.0-beta.ffa4778",
    "tsc-lint": "^0.1.9",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^3.0.6"
  },
  "pnpm": {
    "patchedDependencies": {
      "@arrowood.dev/socket": "patches/@arrowood.dev__socket.patch"
    }
  }
}
